{

    "manifest_version": 2,
    "name": "MediumTamer",
    "version": "0.1",

    "description": "a way to overcome Midium articles limitations ",

    "icons":{"128" : "icon_128.png"},
    "browser_action":{
        "default_icon": "icon_90.png",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"],

    "content_scripts": [
      {
        "matches": ["*://*.medium.com/*"],
        "js": [ "/js/js.cookie.js", "/js/test.js" ]
      }
    ]

  }