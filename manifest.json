{
  "name": "MediumTamer",
  "description": "a way to overcome Midium articles limitations",
  "version": "1.0",
  "manifest_version": 3,

  
  "permissions": ["storage","activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/icon_128.png"
    }
  },

  "icons": {
    "128": "/images/icon_128.png"
  }
}